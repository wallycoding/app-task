{
  "name": "taskapp",
  "version": "1.0.0",
  "description": "Tasks application.",
  "main": "./app/main.js",
  "scripts": {
    "start": "webpack serve --mode=development",
    "build": "webpack --mode=production",
    "dist": "webpack --mode=production && electron-builder",
    "pack": "webpack --mode=production && electron-builder --dir",
    "dist-linux": "webpack --mode=production && electron-builder --linux deb tar.xz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wallycoding/app-task.git"
  },
  "keywords": [
    "react",
    "electron",
    "javascript",
    "webpack",
    "babel",
    "mongodb",
    "api"
  ],
  "author": "wallycoding",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^8.0.0",
    "css-loader": "^5.1.3",
    "electron": "^12.0.1",
    "electron-builder": "^22.10.5",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.27.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@electron/remote": "^1.0.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "axios": "^0.21.1",
    "electron-is-dev": "^2.0.0",
    "mongoose": "^5.12.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0"
  },
  "bugs": {
    "url": "https://github.com/wallycoding/app-task/issues"
  },
  "homepage": "https://github.com/wallycoding/app-task/#readme",
  "build": {
    "productName":"App Task",
    "appId": "com.wally.productions.apptask",
    "target": "NSIS",
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    }
  }
}